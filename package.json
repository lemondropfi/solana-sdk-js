{
  "name": "@lemondropfi/solana-sdk",
  "version": "1.0.5",
  "description": "A JavaScript SDK for easily adding Lemondrop roundups to your Solana dApp",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sol",
    "solana",
    "sdk",
    "lemondrop",
    "finance",
    "defi",
    "javascript",
    "typescript",
    "jupiter",
    "swap",
    "token",
    "crypto",
    "blockchain",
    "web3",
    "dapp"
  ],
  "author": "LX Research Labs, Inc.",
  "license": "See LICENSE",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^22.15.29",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemondropfi/solana-sdk-js.git"
  }
}